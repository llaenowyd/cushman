{
  "name": "@a110/rito",
  "version": "0.0.1",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "fill-style-templates": "ts-node -P tsconfig.tsnode.json scripts/tsnode/fill-style-templates.ts templates dist/styles",
    "typecheck": "tsc",
    "build:prod": "vite build",
    "build": "run-s typecheck build:prod fill-style-templates",
    "build:watch": "nodemon --watch src -e ts,tsx,css --exec \"yarn build\"",
    "preview": "vite preview",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "dependencies": {
    "react": "18.x",
    "react-dom": "18.x"
  },
  "devDependencies": {
    "@node-minify/clean-css": "^6.2.0",
    "@node-minify/core": "^6.2.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "hash-generator": "^0.1.0",
    "json": "^11.0.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.7",
    "vite-plugin-dts": "^1.1.1"
  },
  "peerDependencies": {},
  "files": [
    "dist"
  ],
  "main": "./dist/rito.umd.js",
  "module": "./dist/rito.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/rito.es.js",
      "require": "./dist/rito.umd.js"
    },
    "./styles/*": "./dist/styles/*.css"
  },
  "sideEffects": false
}
